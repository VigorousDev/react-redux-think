extends ./layout.jade

//- optional (PACE loader)
block pace-loader
  link(rel='stylesheet', href='/css/pace.css')
  link(rel='stylesheet', href='/css/font-awesome.min.css')
  script(type='text/javascript', src='/bower_components/PACE/pace.min.js')

block header
  link(rel="stylesheet", href="ASSET{rubix.css,min}")  
  link(rel="stylesheet", href="ASSET{style.css,min}")

block content
  div#app-preloader
  div#app-container
  //- blueimp-gallery snippet (optional)
  div#blueimp-gallery.blueimp-gallery.blueimp-gallery-controls
    div.slides
    h3.title
    p.description
    a.prev ‹
    a.next ›
    a.close ×
    a.play-pause
    ol.indicator

block stylesheets
